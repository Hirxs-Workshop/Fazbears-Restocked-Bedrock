{
	"format_version": "1.16.0",
	"minecraft:entity": {
	  "description": {
		"identifier": "fr:purple_party_door",
		"is_spawnable": false,
		"is_summonable": true
	  },
	  "component_groups": {
		"despawn_entity": {
			"minecraft:instant_despawn": {}
		},
		"is_valid": {
		},
		"open_front": {
		  "minecraft:variant": {
			"value": 1
		  }
		},
		"open_back": {
		  "minecraft:variant": {
			"value": 2
		  }
		},
		"closed": {
		  "minecraft:variant": {
			"value": 0
		  }
		}
	  },
	  "components": {
		"minecraft:type_family": {
			"family": [
				"book",
				"inanimate"
			]
		},
		
		"minecraft:inside_block_notifier": {
			"block_list": [
				{
					"block": "fr:purple_party_door_block",
					"entered_block_event": {
						"event": "inside_block",
						"target": "self"
					},
					"exited_block_event": {
						"event": "destroy",
						"target": "self"
					}
				}
			]
		},
		"minecraft:collision_box": {
			"height": 0,
			"width": 0
		},
		"minecraft:health": {
			"value": 1,
			"min": 1,
			"max": 1
		},
		"minecraft:fire_immune": {},
		"minecraft:damage_sensor": {
			"triggers": {
				"cause": "all",
				"deals_damage": false
			}
		},
		"minecraft:timer": {
			"looping": false,
			"time": 0.05,
			"time_down_event": {
				"event": "check_validity"
			}
		}
	  },
	  "events": {
		"inside_block": {
			"add": {
				"component_groups": [
					"is_valid"
				]
			},
			"remove": {
				"component_groups": [
					"despawn_entity"
				]
			}
		},
		"check_validity": {
			"filters": {
				"test": "has_component",
				"operator": "!=",
				"value": "is_valid"
			},
			"add": {
				"component_groups": [
					"despawn_entity"
				]
			}
		},
		"destroy": {
			"add": {
				"component_groups": [
					"despawn_entity"
				]
			}
		},
		"open_door": {
		  "add": {
			"component_groups": ["open_front"]
		  },
		  "remove": {
			"component_groups": ["open_back", "closed"]
		  }
		},
		"open_door_back": {
		  "add": {
			"component_groups": ["open_back"]
		  },
		  "remove": {
			"component_groups": ["open_front", "closed"]
		  }
		},
		"close_door": {
		  "add": {
			"component_groups": ["closed"]
		  },
		  "remove": {
			"component_groups": ["open_front", "open_back"]
		  }
		},
		"close_door_back": {
		  "add": {
			"component_groups": ["closed"]
		  },
		  "remove": {
			"component_groups": ["open_front", "open_back"]
		  }
		}
	  }
	}
  }