				},
				"remove": {
					"component_groups": ["freddy_hostile"]
				}
			},
			"freddy_stop_chase": {
				"add": {
					"component_groups": ["freddy_hostile"]
				},
				"remove": {
					"component_groups": ["freddy_chasing", "freddy_scare_cooldown"]
				},
				"queue_command": {
					"command": [
						"tag @s remove played_scare"
					]
				}
			},
			"freddy_set_attack_time_1": {
				"set_property": {
					"freddy:attack_time": "1.0"
				}
			},
			"freddy_set_attack_time_0": {
				"set_property": {
					"freddy:attack_time": "0.0"
				}
			},
			"freddy_kill_player": {
				"queue_command": {
					"command": "kill @p"
				}
			},
			"freddy_hit_particles": {
				"queue_command": {
					"command": [
						"execute at @p positioned ~ ~ ~ run particle minecraft:endrod ~ ~ ~"
					]
				}
			},
			"freddy_start_attack": {
				"add": {
					"component_groups": ["freddy_attacking", "freddy_attack_timer"]
				},
				"remove": {
					"component_groups": ["freddy_chasing", "freddy_hostile"]
				},
				"set_property": {
					"freddy:attack_time": "1.0"
				},
				"queue_command": {
					"command": [
						"say (DEBUG) freddy start attack executed",
						"effect @p slowness 5 5 true",
						"effect @p darkness 5 5 true",
						"ride @p start_riding @s teleport_ride",
						"camerashake add @p 0.05 3.4 rotational",
						"hud @p hide all",
						"execute at @p positioned ~ ~ ~ run particle fr:attack_blood_drip ~ ~0.6 ~",
						"execute at @p positioned ~ ~ ~ run tag @p add freddy_hit"
					]
				}
			},
			"freddy_sitting_player": {
				"queue_command": {
					"command": [
						"ride @p start_riding @s teleport_ride"
					]
				}
			},
			"freddy_end_attack": {
				"remove": {
					"component_groups": ["freddy_attacking", "freddy_attack_timer", "freddy_normal"]
				},
				"add": {
					"component_groups": ["freddy_chasing", "freddy_post_attack"]
				},
				"set_property": {
					"freddy:attack_time": "0.0"
				},
				"queue_command": {
					"command": [
						"say (DEBUG) freddy end attack executed - should be variant 2 now",
						"effect @s slowness 7 10 true",
						"hud @p reset all",
						"execute at @p positioned ~ ~ ~ run particle fr:attack_blood_drip_final ~ ~0.6 ~",
						"kill @p[tag=freddy_hit]",
						"execute at @p positioned ~ ~ ~ run tag @p remove freddy_hit",
						"tag @s remove played_scare"
					]
				}
			},
			"freddy_reset_scare": {
				"queue_command": {
					"command": [
						"say (DEBUG) freddy scare cooldown finished",
						"tag @s remove played_scare"
					]
				},
				"remove": {
					"component_groups": ["freddy_scare_cooldown"]
				}
			},
			"freddy_clean_blood": {
				"remove": {
					"component_groups": ["freddy_post_attack"]
				},
				"add": {
					"component_groups": ["freddy_normal"]
				},
				"queue_command": {
					"command": [
						"say (DEBUG) freddy blood cleaned, back to variant 0",
						"execute at @p positioned ~ ~ ~ run particle minecraft:splash ~ ~1 ~ 0.5 0.5 0.5 0.1 10"
					]
				}
			},
			"freddy_check_for_player": {
				"sequence": [
					{
						"filters": {
							"all_of": [
								{ "test": "has_target" },
								{ "test": "has_tag", "subject": "self", "value": "played_scare", "operator": "not" }
							]
						},
						"add": {
							"component_groups": ["freddy_chasing", "freddy_scare_cooldown"]
						},
						"remove": {
							"component_groups": ["freddy_hostile"]
						},
						"queue_command": {
							"command": [
								"execute at @s run playsound fnaf1_freddy_scare @a[r=16] ~ ~ ~ 1 1",
								"tag @s add played_scare"
							]
						}
					},
					{
						"filters": {
							"test": "has_target",
							"operator": "not"
						},
						"add": {
							"component_groups": ["freddy_hostile"]
						},
						"remove": {
							"component_groups": ["freddy_chasing"]
						},
						"queue_command": {
							"command": [
