{
	"format_version": "1.21.30",
	"minecraft:block": {
	  "description": {
		"identifier": "fr:entrance_door_block",
		"menu_category": {
                "category": "construction",
          		"group": "itemGroup.name.door"
		},
		"traits": {
		  "minecraft:placement_direction": {
			"enabled_states": ["minecraft:cardinal_direction"]
		  }
		},
		"states": {
		  "fr:open_bit": ["closed", "open_left", "open_right"],
		  "fr:upper_block_bit": [false, true],
		  "fr:bottom_block_bit": [false, true],
		  "fr:middle_block_bit": [false, true],
		  "fr:destroyed": [false, true],
		  "fr:value": [
			0, 1, 2, 3, 4, 5, 6, 7, 8
		  ]
		}
	  },
	  "permutations": [
		{
			"condition": "query.block_state('minecraft:cardinal_direction') == 'east' && query.block_state('fr:open_bit') == 'closed'",
			"components": {
			  "minecraft:selection_box": {
				"origin": [-1, 0, -8],
				"size": [2, 16, 16]
			  },
			  "minecraft:collision_box": {
				"origin": [-1, 0, -8],
				"size": [2, 16, 16]
			  },
			  "minecraft:geometry": "geometry.fr_door_inv"
			}
		  },
		  {
			"condition": "query.block_state('minecraft:cardinal_direction') == 'west' && query.block_state('fr:open_bit') == 'closed'",
			"components": {
			  "minecraft:selection_box": {
				"origin": [-1, 0, -8],
				"size": [2, 16, 16]
			  },
			  "minecraft:collision_box": {
				"origin": [-1, 0, -8],
				"size": [2, 16, 16]
			  },
			  "minecraft:geometry": "geometry.fr_door_inv"
			}
		  },
		


		{
		  "condition": "query.block_state('minecraft:cardinal_direction') == 'north' && query.block_state('fr:open_bit') == 'open_left'",
		  "components": {
			"minecraft:selection_box": {
				"origin": [6, 0, -8],
				"size": [2, 16, 16]
			},
			"minecraft:collision_box": {
				"origin": [6, 0, -8],
				"size": [2, 16, 16]
			},
			"minecraft:geometry": "geometry.fr_door_inv"
		  }
		},
		{
		  "condition": "query.block_state('minecraft:cardinal_direction') == 'north' && query.block_state('fr:open_bit') == 'open_right'",
		  "components": {
			"minecraft:selection_box": {
				"origin": [-8, 0, -8],
				"size": [2, 16, 16]
			},
			"minecraft:collision_box": {
				"origin": [-8, 0, -8],
				"size": [2, 16, 16]
			},
			"minecraft:geometry": "geometry.fr_door_inv"
		  }
		},




		{
		  "condition": "query.block_state('minecraft:cardinal_direction') == 'south' && query.block_state('fr:open_bit') == 'open_left'",
		  "components": {
			"minecraft:selection_box": {
				"origin": [-8, 0, -8],
				"size": [2, 16, 16]
			},
			"minecraft:collision_box": {
				"origin": [-8, 0, -8],
				"size": [2, 16, 16]
			},
			"minecraft:geometry": "geometry.fr_door_inv"
		  }
		},
		{
		  "condition": "query.block_state('minecraft:cardinal_direction') == 'south' && query.block_state('fr:open_bit') == 'open_right'",
		  "components": {
			"minecraft:selection_box": {
				"origin": [6, 0, -8],
				"size": [2, 16, 16]
			},
			"minecraft:collision_box": {
				"origin": [6, 0, -8],
				"size": [2, 16, 16]
			},
			"minecraft:geometry": "geometry.fr_door_inv"
		  }
		},
		{
		  "condition": "query.block_state('minecraft:cardinal_direction') == 'east' && query.block_state('fr:open_bit') == 'open_left'",
		  "components": {
			"minecraft:selection_box": {
				"origin": [-8, 0, -8],
				"size": [16, 16, 2]
			},
			"minecraft:collision_box": {
				"origin": [-8, 0, -8],
				"size": [16, 16, 2]
			},
			"minecraft:geometry": "geometry.fr_door_inv"
		  }
		},
		{
		  "condition": "query.block_state('minecraft:cardinal_direction') == 'east' && query.block_state('fr:open_bit') == 'open_right'",
		  "components": {
			"minecraft:selection_box": {
				"origin": [-8, 0, 6],
				"size": [16, 16, 2]
			},
			"minecraft:collision_box": {
				"origin": [-8, 0, 6],
				"size": [16, 16, 2]
			},
			"minecraft:geometry": "geometry.fr_door_inv"
		  }
		},
		{
			"condition": "query.block_state('minecraft:cardinal_direction') == 'west' && query.block_state('fr:open_bit') == 'open_left'",
			"components": {
			  "minecraft:selection_box": {
				  "origin": [-8, 0, 6],
				  "size": [16, 16, 2]
			  },
			  "minecraft:collision_box": {
				  "origin": [-8, 0, 6],
				  "size": [16, 16, 2]
			  },
			  "minecraft:geometry": "geometry.fr_door_inv"
			}
		  },
		  {
			"condition": "query.block_state('minecraft:cardinal_direction') == 'west' && query.block_state('fr:open_bit') == 'open_right'",
			"components": {
			  "minecraft:selection_box": {
				  "origin": [-8, 0, -8],
				  "size": [16, 16, 2]
			  },
			  "minecraft:collision_box": {
				  "origin": [-8, 0, -8],
				  "size": [16, 16, 2]
			  },
			  "minecraft:geometry": "geometry.fr_door_inv"
			}
		  },
		{
		  "condition": "query.block_state('minecraft:cardinal_direction') == 'north' && !query.block_state('fr:open_bit')",
		  "components": {
			"minecraft:transformation": {
			  "rotation": [0, 0, 0]
			}
		  }
		},
		{
		  "condition": "query.block_state('minecraft:cardinal_direction') == 'south' && !query.block_state('fr:open_bit')",
		  "components": {
			"minecraft:transformation": {
			  "rotation": [0, 180, 0]
			}
		  }
		},
		{
		  "condition": "query.block_state('minecraft:cardinal_direction') == 'east' && !query.block_state('fr:open_bit')",
		  "components": {
			"minecraft:transformation": {
			  "rotation": [0, 270, 0]
			}
		  }
		},
		{
		  "condition": "query.block_state('minecraft:cardinal_direction') == 'west' && !query.block_state('fr:open_bit')",
		  "components": {
			"minecraft:transformation": {
			  "rotation": [0, 90, 0]
			}
		  }
		},
		{
		  "condition": "!q.block_state('fr:upper_block_bit')",
		  "components": {
			"minecraft:custom_components": [
			  "fr:en_on_place",
			  "fr:en_on_interact",
			  "fr:en_on_tick"
			],
			"minecraft:material_instances": {
			  "*": {
				"texture": "dirt",
				"render_method": "alpha_test"
			  }
			}
		  }
		},
		{
		  "condition": "q.block_state('fr:upper_block_bit')",
		  "components": {
			"minecraft:custom_components": ["fr:en_on_interact", "fr:en_on_tick"],
			"minecraft:material_instances": {
			  "*": {
				"texture": "dirt",
				"render_method": "alpha_test"
			  }
			}
		  }
		}
	  ],
	  "components": {
		"minecraft:custom_components": ["fr:en_on_place", "fr:en_on_interact", "fr:en_on_tick", "fr:en_on_player_destroy"],
		"minecraft:collision_box": {
		  "origin": [-8, 0, -2],
		  "size": [16, 16, 4]
		},
		"minecraft:selection_box": {
		  "origin": [-8, 0, -2],
		  "size": [16, 16, 4]
		},
		"minecraft:tick": {
		  "interval_range": [3, 3]
		},
		"minecraft:destructible_by_mining": {
		  "seconds_to_destroy": 3
		},
		"minecraft:destructible_by_explosion": {
		  "explosion_resistance": 3
		},
		"minecraft:geometry": {
		  "identifier": "geometry.fr_door_inv"
		},
		"minecraft:material_instances": {
		  "*": {
			"texture": "dirt",
			"render_method": "alpha_test"
		  }
		}
	  }
	}
  }