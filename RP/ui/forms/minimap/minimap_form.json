  "offsetable_panel": {
    "type": "collection_panel",
    "anchor_from": "top_left",
    "anchor_to": "center",
    "controls": [
      {
        "$parent_name": {
          "type": "panel",
          "offset": "#offset",
          "visible": false,
          "property_bag": {
            "#big_offset_x_0": [0, 0],
