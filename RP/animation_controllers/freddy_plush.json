{
	"format_version": "1.10.0",
	"animation_controllers": {
		"controller.animation.freddy_plush": {
			"initial_state": "idle",
			"states": {
				"idle": {
					"animations": [
						"fr_idle"
					],
					"transitions": [
						{
							"clicking": "query.variant == 1"
						}
					]
				},
				"clicking": {
					"animations": [
						"fr_clicked"
					],
					"transitions": [
						{
							"idle": "query.variant == 0"
						}
					]
				}
			}
		}
	}
}
