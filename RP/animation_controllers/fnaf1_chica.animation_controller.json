{
	"format_version": "1.10.0",
	"animation_controllers": {
		"controller.animation.fnaf1_chica.base": {
			"states": {
				"default": {
					"animations": [
						{
							"default": "query.variant != 1"
						},
						{
							"default_bloody": "query.variant == 1"
						}
					],
					"blend_transition": 0.2
				}
			}
		},
		"controller.animation.fnaf1_chica.movement": {
			"states": {
				"default": {
					"transitions": [
						{
							"attack": "variable.attack_time > 0"
						},
						{
							"run": "variable.attack_time <= 0 && (query.is_moving || query.modified_move_speed > 0.02) && query.modified_move_speed >= 0.6"
						},
						{
							"walk": "variable.attack_time <= 0 && (query.is_moving || query.modified_move_speed > 0.02) && query.modified_move_speed < 0.6"
						},
						{
							"idle": "variable.attack_time <= 0 && !query.is_moving && query.modified_move_speed <= 0.02"
						}
					],
					"blend_transition": 0.3
				},
				"walk": {
					"animations": [
						{
							"walk": "query.variant != 1"
						},
						{
							"walk_bloody": "query.variant == 1"
						},
						"look_at_target"
					],
					"transitions": [
						{
							"attack": "variable.attack_time > 0"
						},
						{
							"run": "variable.attack_time <= 0 && query.modified_move_speed >= 0.6"
						},
						{
							"idle": "variable.attack_time <= 0 && !query.is_moving && query.modified_move_speed <= 0.02"
						}
					],
					"blend_transition": 0.1
				},
				"run": {
					"animations": [
						{
							"run": "query.variant != 1"
						},
						{
							"run_bloody": "query.variant == 1"
						},
						"look_at_target"
					],
					"transitions": [
						{
							"attack": "variable.attack_time > 0"
						},
						{
							"walk": "variable.attack_time <= 0 && (query.is_moving || query.modified_move_speed > 0.02) && query.modified_move_speed < 0.5"
						},
						{
							"idle": "variable.attack_time <= 0 && !query.is_moving && query.modified_move_speed <= 0.02"
						}
					],
					"blend_transition": 0.2
				},
				"idle": {
					"animations": [
						{
							"idle": "query.variant != 1"
						},
						{
							"idle_bloody": "query.variant == 1"
						}
					],
					"transitions": [
						{
							"attack": "variable.attack_time > 0"
						},
						{
							"run": "variable.attack_time <= 0 && (query.is_moving || query.modified_move_speed > 0.02) && query.modified_move_speed >= 0.6"
						},
						{
							"walk": "variable.attack_time <= 0 && (query.is_moving || query.modified_move_speed > 0.02) && query.modified_move_speed < 0.6"
						}
					],
					"blend_transition": 0.1
				},
				"attack": {
					"animations": [
						"attack"
					],
					"transitions": [
						{
							"idle": "query.all_animations_finished && variable.attack_time <= 0 && !query.is_moving && query.modified_move_speed <= 0.02"
						},
						{
							"run": "query.all_animations_finished && variable.attack_time <= 0 && (query.is_moving || query.modified_move_speed > 0.02) && query.modified_move_speed >= 0.6"
						},
						{
							"walk": "query.all_animations_finished && variable.attack_time <= 0 && (query.is_moving || query.modified_move_speed > 0.02) && query.modified_move_speed < 0.6"
						}
					],
					"blend_transition": 0.8
				}
			}
		}
	}
}
